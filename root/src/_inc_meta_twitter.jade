// Twitter: Summary Card
// https://dev.twitter.com/docs/cards/markup-reference
// *Some properties have Open Graph fallback to avoid duplication*
meta(name='twitter:card', content='summary')
meta(name='twitter:site', content=twitter.creator)
meta(name='twitter:site:id', content=twitter.creator_id)
meta(name='twitter:creator', content=twitter.creator)
meta(name='twitter:creator:id', content=twitter.creator_id)
